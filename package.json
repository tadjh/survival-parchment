    "@dotenv-run/esbuild": "^1.3.5",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "esbuild": "0.16.5",
    "esbuild-plugin-postcss2": "^0.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@deanc/esbuild-plugin-postcss": "^1.0.2",
